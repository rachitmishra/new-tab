{
  "manifest_version": 2,

  "name": "Quotes",
  "description": "This extension pull quotes from wiki quotes and shows on new page",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions" : ["management"],
  "chrome_url_overrides" : {
        "newtab": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://en.wikiquote.org; object-src 'self'"

}
